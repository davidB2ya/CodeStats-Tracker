{
  "name": "codestats-tracker",
  "displayName": "CodeStats Tracker",
  "description": "CodeStats Tracker is an extension for Visual Studio Code that allows you to track your coding time, view advanced productivity statistics",
  "version": "0.1.2",
  "publisher": "codestats-tracker",
  "author": {
    "name": "David Bedoya Hern√°ndez",
    "email": "dbedoya0408@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/davidB2ya/CodeStats-Tracker.git"
  },
  "icon": "icon.png",
  "license": "MIT",
  "keywords": [
    "codestats",
    "tracker",
    "productivity",
    "statistics",
    "coding time",
    "visual studio code"
  ],
  "homepage": "https://github.com/davidB2ya/CodeStats-Tracker",
  "bugs": {
    "url": "https://github.com/davidB2ya/CodeStats-Tracker/-/issues"
  },
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codestats.viewDashboard",
        "title": "View CodeStats Dashboard",
        "category": "CodeStats"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "codestats.viewDashboard",
          "title": "CodeStats: View Dashboard"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "codestats-sidebar",
          "title": "CodeStats Tracker",
          "icon": "$(dashboard)"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.101.0",
    "@types/mocha": "^10.0.10",
    "@types/glob": "^8.1.0",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "glob": "^8.1.0",
    "typescript": "^5.8.3",
    "ts-loader": "^9.5.2",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  },
  "dependencies": {
    "chart.js": "^4.4.0"
  }
}
